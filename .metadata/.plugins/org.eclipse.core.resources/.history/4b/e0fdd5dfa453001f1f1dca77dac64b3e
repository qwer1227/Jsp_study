public class DeptDao {

	/**
	 * 모든 부서정보를 반환한다
	 * @return 부서목록
	 * @throws SQLException
	 */
	public List<DeptList> findAllDepts() throws SQLException{
		List<DeptList> depts = new ArrayList<DeptList>();
		
		return depts;
	}
}
